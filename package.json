{
  "name": "presence.toughlovearena.com",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "ps": "netstat -lnp | grep 2999",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "prestart": "npm-run-all lint build",
    "start": "node dist/index.js",
    "watch": "nodemon",
    "bg": "forever start dist/index.js",
    "kill": "forever stop dist/index.js",
    "update": "git pull && npm i",
    "rebuild": "npm-run-all update build kill bg",
    "test": "jest --detectOpenHandles",
    "ci-test": "jest --detectOpenHandles --runInBand"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fs": "0.0.1-security"
  },
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.13",
    "forever": "^3.0.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
